{
  "name": "vue-clerk-nuxt",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node ./scripts/watch.mjs",
    "postinstall": "nuxt-module-build prepare",
    "build": "nuxt-module-build build && pnpm run move",
    "move": "rm -rf ../vue-clerk/dist/nuxt && cp -R dist ../vue-clerk/dist/nuxt"
  },
  "dependencies": {
    "@clerk/backend": "^1.21.5",
    "@clerk/types": "^4.40.1",
    "@nuxt/kit": "^3.13.2",
    "@nuxt/schema": "^3.13.2",
    "h3": "^1.12.0",
    "vue-clerk": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.4.2",
    "@nuxt/module-builder": "0.8.4",
    "@types/node": "^22.5.1",
    "nodemon": "^3.1.4",
    "nuxt": "3.13.2"
  }
}
